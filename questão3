#include <stdio.h>
int main()
{
    int vet1[10], vet2[10], inter[20], i;
    
    printf("Informe os valores do primeiro vetor: ");
    for(i = 0; i < 10; i++){
    scanf("%i", &vet1[i]);
    }
    printf("\nInforme os valores do segundo vetor: ");
    for(i = 0; i < 10; i++){
    scanf("%i", &vet2[i]);
}
    printf("\nIntercessao: ");
    for(int i=0; i < 10;i++){
        for(int j=0;j < 10;j++){
            if(vet1[i] == vet2[j]){
                printf("%i, ", vet1[i]);
            }
           
        }
    }


    return 0;
}
